    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "GoogleDriveTestUtility",
            "request": "launch",
            "mainClass": "br.adv.cra.util.GoogleDriveTestUtility",
            "projectName": "cra-backend"
        },
        {
            "type": "java",
            "name": "PasswordGenerator",
            "request": "launch",
            "mainClass": "br.adv.cra.util.PasswordGenerator",
            "projectName": "cra-backend"
        },
        {
            "type": "java",
            "name": "CraBackendApplication",
            "request": "launch",
            "mainClass": "br.adv.cra.CraBackendApplication",
            "projectName": "cra-backend",
            "vmArgs": [
                "-Djavax.net.ssl.trustStore=",
                "-Djavax.net.ssl.keyStore=",
                "-Djdk.tls.client.protocols=TLSv1.2",
                "-Dserver.ssl.enabled=false"
            ]
        },
        {
            "type": "java",
            "name": "CraBackendApplication (Dev Profile)",
            "request": "launch",
            "mainClass": "br.adv.cra.CraBackendApplication",
            "projectName": "cra-backend",
            "vmArgs": [
                "-Dspring.profiles.active=dev",
                "-Djavax.net.ssl.trustStore=",
                "-Djavax.net.ssl.keyStore=",
                "-Djdk.tls.client.protocols=TLSv1.2",
                "-Dserver.ssl.enabled=false",
                "-DGOOGLE_DRIVE_CLIENT_ID=514473695163-on6hdl2unuvnc6qgjf6an10u0v4i0kcv.apps.googleusercontent.com",
                "-DGOOGLE_DRIVE_CLIENT_SECRET=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",
                "-DGOOGLE_DRIVE_REDIRECT_URI=http://localhost:8081/Callback",
                "-DGOOGLE_DRIVE_FOLDER_ID=1cXM4Pq2FVXh8dWoRICDNMjMIZOrNa9gc"
            ]
        },
        {
            "type": "java",
            "name": "CraBackendApplication (No SSL)",
            "request": "launch",
            "mainClass": "br.adv.cra.CraBackendApplication",
            "projectName": "cra-backend",
            "vmArgs": [
                "-Djavax.net.ssl.trustStore=",
                "-Djavax.net.ssl.keyStore=",
                "-Djdk.tls.client.protocols=TLSv1.2",
                "-Dserver.ssl.enabled=false",
                "-DGOOGLE_DRIVE_CLIENT_ID=514473695163-on6hdl2unuvnc6qgjf6an10u0v4i0kcv.apps.googleusercontent.com",
                "-DGOOGLE_DRIVE_CLIENT_SECRET=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",
                "-DGOOGLE_DRIVE_REDIRECT_URI=http://localhost:8081/Callback",
                "-DGOOGLE_DRIVE_FOLDER_ID=1cXM4Pq2FVXh8dWoRICDNMjMIZOrNa9gc"
            ]
        },
        {
            "type": "java",
            "name": "CraBackendApplication (Debug)",
            "request": "launch",
            "mainClass": "br.adv.cra.CraBackendApplication",
            "projectName": "cra-backend",
            "vmArgs": [
                "-Dspring.profiles.active=dev",
                "-Djavax.net.ssl.trustStore=",
                "-Djavax.net.ssl.keyStore=",
                "-Djdk.tls.client.protocols=TLSv1.2",
                "-Dserver.ssl.enabled=false",
                "-Djava.security.manager=allow",
                "-DGOOGLE_DRIVE_CLIENT_ID=514473695163-on6hdl2unuvnc6qgjf6an10u0v4i0kcv.apps.googleusercontent.com",
                "-DGOOGLE_DRIVE_CLIENT_SECRET=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",
                "-DGOOGLE_DRIVE_REDIRECT_URI=http://localhost:8081/Callback",
                "-DGOOGLE_DRIVE_FOLDER_ID=1cXM4Pq2FVXh8dWoRICDNMjMIZOrNa9gc"
            ]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}"
        }
    ]
}